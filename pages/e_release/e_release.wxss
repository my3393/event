/* pages/e_release/e_release.wxss */
page{
  background: #f2f2f2;
}
.event{
  margin-left: 20rpx;
  color: #898989;
  font-size: 24rpx;
  line-height: 60rpx;
}
.infor{
  background: #fff;
  padding: 0 20rpx;
  box-sizing: border-box;
  border-bottom: 4rpx dotted #f2f2f2;
  line-height: 100rpx;
}
.inform{
  display: inline-block;
  vertical-align: middle;
  margin-right: 20rpx;
}
.inforin{
  display: inline-block;
  vertical-align: middle;
}
.inforin input{
  width: 500rpx;
  letter-spacing: 2rpx;
}
.forin{
  margin-left: 30rpx;
}
.enter{
  float: right;
}
.enter image{
  width: 14rpx;
  height: 26rpx;
}
.submit{
  width: calc(100% - 40rpx);
  margin-left: 20rpx;
  border-radius: 40rpx;
  height: 80rpx;
  background: #F861AA;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  margin-top: 40rpx;
}
.pick{
  display: inline-block;
  color: #666;
  width: 500rpx;
}
.jgtx{
  background: #fff;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  border-bottom: 4rpx dotted #f2f2f2;
}
.jgtu{
  display:inline-block;
}
.photo>image{
  width: 134rpx;
  height: 100rpx;
  
}
.photo{
  display:inline-block;
  float:right;
  height: 100rpx;
  margin-top: 16rpx;
}
.pho{
  height: 130rpx;
  line-height: 130rpx;
}
.xfcf{
  height: 230rpx;
}
.xfdf{
  height: 260rpx;
}
.xfcf image{
  width: 90rpx;
  height: 90rpx;
}
.evedetil{
  width: 100%;
  height: 130rpx;
  line-height: 40rpx!important;
  overflow-x:visible;
}


.ilebels{
    width: 100%;
    height: 600rpx;
    background-color: white;
    position: fixed;
    bottom: 0;
    left: 0;
}
.tops{
    display: flex;
    justify-content: space-between;
    padding: 0 20rpx;
    box-sizing: border-box;
    line-height: 80rpx;
}
.quxiao{
    color: #999999;
    font-size: 32rpx;
}
.sure{
    color: #1AAD19;
    font-size: 32rpx;
}
.ilablein{
    width: 100%;
    height: 520rpx;
    overflow: scroll;
    padding: 20rpx;
    box-sizing: border-box;
    -webkit-overflow-scrolling: touch;
}
.ilablein text{
    display: inline-block;
    width: 200rpx;
    line-height: 60rpx;
    border-radius: 6rpx;
    border: 1px solid #808080;
    text-align: center;
    margin-left: 25rpx;
    margin-bottom: 20rpx;
    color: #808080;
    font-size: 26rpx;
}
.iact{
    color: #fe5ba9 !important;
    border: 1px solid #fe5ba9 !important;
}
.vmask{
    width: 80rpx;
    height: 80rpx;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 99rpx;
    background: rgba(0,0,0,0.3);
    text-align: center;
    color: white;
    line-height: 80rpx;
    font-size: 20rpx;
}
.mask{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.2);
    z-index: 12;
    /* display: none;  */
}