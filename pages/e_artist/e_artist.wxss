 background: #fff;
 text-align: right;
 margin-top: -10rpx;
}
.xia{
  float: right;
  width: 36rpx;
  margin-right: 20rpx;
  height: 16rpx;
}
.xia image{
  width: 100%;
  height: 100%;
}
.mwrap{
	width: 100%;
	position: fixed;
	bottom: 0;
	left: 0;
}
.mtitle{

	height: 80rpx;
	line-height: 80rpx;
	background-color: #e6e6e6 !important;
	width: 100%;
	font-size: 30rpx;
	color: #333;
	text-align: center;
}
.province{
	background-color: white;
	width: 100%;
	height: 520rpx;
	overflow: scroll;
	-webkit-overflow-scrolling: touch;
}
.province view{
	line-height: 80rpx;
	padding-left: 40rpx;
	box-sizing: border-box;
	border-bottom: 1px solid #DDDDDD;
	font-size: 28rpx;
	color: #333;
}
.province_active{
	background-color: #FE5BA9;
	color: white !important;
}
.next{
   width: calc(100% - 40rpx);
  margin-left: 20rpx;
  border-radius: 40rpx;
  height: 80rpx;
  background: #F861AA;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  margin-top: 40rpx;
}
.xse{
